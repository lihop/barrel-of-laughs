[application]

config/name="Barrel of Laughs"
config/description="Hide and go seek game where the player is the seeker and the hiding NPC laughs at you."
run/main_scene="res://gui/main_menu/main_menu.tscn"
config/icon="res://icon.png"

[autoload]

Events="*res://singletons/events.gd"
